### Do not edit -- Generated by 'configure --with-whatever' from Makefile.in
### RtMidi tests Makefile - for various flavors of unix

PROGRAMS = sm_echo
RM = /bin/rm
SRC_PATH = ..
INCLUDE = ..
OBJECT_PATH = Release
vpath %.o $(OBJECT_PATH)

OBJECTS	=	RtMidi.o

CC       = g++
DEFS     =   -D__MACOSX_CORE__
CFLAGS   = -O3 -Wall
CFLAGS  += -I$(INCLUDE)
LIBRARY  = -framework CoreMIDI -framework CoreFoundation -framework CoreAudio

%.o : $(SRC_PATH)/%.cpp
	$(CC) $(CFLAGS) $(DEFS) -c $(<) -o $(OBJECT_PATH)/$@

all : $(PROGRAMS)

sm_echo : sm_echo.cpp $(OBJECTS)
	$(CC) $(CFLAGS) $(DEFS) -o sm_echo sm_echo.cpp $(OBJECT_PATH)/RtMidi.o $(LIBRARY)

clean : 
	-rm $(OBJECT_PATH)/*.o
	-rm $(PROGRAMS) *.exe
	-rm -f *~

strip : 
	strip $(PROGRAMS)
